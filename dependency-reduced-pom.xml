        <exclusion>
          <artifactId>spring-integration-test-support</artifactId>
          <groupId>org.springframework.integration</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.springframework.data</groupId>
      <artifactId>spring-data-jpa</artifactId>
      <version>3.1.4</version>
